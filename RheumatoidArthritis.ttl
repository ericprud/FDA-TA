:RadiographicResponseEndpoint 
    rdfs:subClassOf 
        core:EfficacyEndpoint ,
        [ owl:onProperty core:evaluates ; owl:someValuesFrom :RadiographicScoreOutcomeAssessment ] ;
    rdfs:label "Radiographic response" ;
    skos:definition """An RA outcome measure that uses radiographs as the simplest and least expensive permanent record of the cumulative joint damage caused by the disease.""" .
:RadiographicScoreOutcomeAssessment 
    rdfs:subClassOf 
        core:SingleOutcomeAssessment ,
        [ owl:onProperty core:beforeIntervention ; owl:someValuesFrom :RadiographicScoreAssessment ] ,
        [ owl:onProperty core:afterIntervention ; owl:someValuesFrom :RadiographicScoreAssessment ] .
:RadiographicScoreAssessment 
    rdfs:subClassOf 
        :Assessment ,
        [ owl:onProperty core:hasObservation ; owl:someValuesFrom :ErosionScoreObservation ] ,
        [ owl:onProperty core:hasObservation ; owl:someValuesFrom :JointSpaceNarrowingScoreObservation ] ;
    rdfs:label "Radiographic Score" ;
    skos:definition """It is a calculation that supports Radiographic Response outcome measure and is derived from  two components: joint space narrowing and erosion. Examples of  most common scoring methods are  Sharp score, Van der Heijde score, modified Van der Heijde score and Genant score.""" .
:ErosionScoreObservation 
    rdfs:subClassOf 
        core:SignsAndSymptoms ,
        [ owl:onProperty bridg:PerformedObservation.resultedPerformedObservationResult ; owl:someValuesFrom bridg:PerformedClinicalResult ] ,
        [ owl:onProperty bridg:PerformedActivity.instantiatedDefinedActivity ; owl:hasValue :DefinedErosionScoreObservation ] ;
    rdfs:label "Erosion Score" ;
    skos:definition """A result of a calculation method based on the radiographic obervation and measurments of the joints' damage. """ .
:DefinedErosionScoreObservation rdfs:subClassOf bridg:DefinedObservation ;
    rdfs:label "Erosion Score" ;
    skos:definition """A result of a calculation method based on the radiographic obervation and measurments of the joints' damage. """ .

:JointSpaceNarrowingScoreObservation 
    rdfs:subClassOf 
        core:SignsAndSymptoms ,
        [ owl:onProperty bridg:PerformedObservation.resultedPerformedObservationResult ; owl:someValuesFrom bridg:PerformedClinicalResult ] ,
        [ owl:onProperty bridg:PerformedActivity.instantiatedDefinedActivity ; owl:hasValue :DefinedJointSpaceNarrowingScoreObservation ] ;
    rdfs:label "Joint Space Narrowing (JSN) Score" ;
    skos:definition """A result of a calculation method based on the radiographic obervation and measurments of the joints' damage. """ .
:DefinedJointSpaceNarrowingScoreObservation rdfs:subClassOf bridg:DefinedObservation ;
    rdfs:label "Joint Space Narrowing (JSN) Score" ;
    skos:definition """A result of a calculation method based on the radiographic obervation and measurments of the joints' damage. """ .

:Organizer a owl:Class . # organizer for the RheumatoidArthritis Therapeutic Area .
:Subject rdfs:subClassOf :Organizer .
:Protocol rdfs:subClassOf :Organizer .
:AllEndpoints rdfs:subClassOf :Organizer ;
    owl:equivalentClass [ owl:unionOf (
        :DiseaseActivityScoreEndpoint 
        :AmericanCollegeOfRheumatologyEndpoint 
        :PhysicalFunctionEndpoint 
        :HealthRelatedQualityOfLifeEndpoint 
        :RadiographicResponseEndpoint 
    ) ] .

:Protocol 
    rdfs:subClassOf :Organizer, core:TAProtocol ,
        [ owl:onProperty :hasEndpoint ; owl:someValuesFrom :AllEndpoints ] .


