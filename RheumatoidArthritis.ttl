:DAS28_Assessment 
    rdfs:subClassOf 
        :Assessment ,
        [ owl:someValuesFrom core:beforeIntervention ; owl:someValuesFrom :SwollenJointCountObservation ] ,
        [ owl:someValuesFrom core:beforeIntervention ; owl:someValuesFrom :TenderJointCountObservation ] ,
        [ owl:someValuesFrom core:beforeIntervention ; owl:someValuesFrom :CReactiveProteinObservation ] ,
        [ owl:someValuesFrom core:beforeIntervention ; owl:someValuesFrom :ErythrociteSedimentationRateObservation ] ,
