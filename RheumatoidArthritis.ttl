:PhysicalFunctionEndpoint 
    rdfs:subClassOf 
        core:EfficacyEndpoint ,
        [ owl:onProperty core:evaluates ; owl:someValuesFrom :HAQ-DI_OutcomeAssessment ] ;
    rdfs:label "PF: Physical Function" ;
    skos:definition """10 items assess physical functio""" .
:HAQ-DI_OutcomeAssessment 
    rdfs:subClassOf 
        core:SingleOutcomeAssessment ,
        [ owl:onProperty core:beforeIntervention ; owl:someValuesFrom :HAQ-DI_Assessment ] ,
        [ owl:onProperty core:afterIntervention ; owl:someValuesFrom :HAQ-DI_Assessment ] .
:HealthRelatedQualityOfLifeEndpoint 
    rdfs:subClassOf 
        core:EfficacyEndpoint ,
        [ owl:onProperty core:evaluates ; owl:someValuesFrom :HRQoL_OutcomeAssessment ] ;
    rdfs:label "Health Related Quality Of Life (HRQoL)" ;
    skos:definition """Health-related quality of life (HRQOL) and its determinants encompass those aspects of overall quality of life that can be clearly shown to affect health—either physical or mental. On the individual level, this includes physical and mental health perceptions and their correlates—including health risks and conditions, functional status, social support, and socioeconomic status. On the community level, HRQOL includes resources, conditions, policies, and practices that influence a population’s health perceptions and functional status""" .
:HRQoL_OutcomeAssessment 
    rdfs:subClassOf 
        core:SingleOutcomeAssessment ,
        [ owl:onProperty core:beforeIntervention ; owl:someValuesFrom :SF-36_Assessment ] ,
        [ owl:onProperty core:afterIntervention ; owl:someValuesFrom :SF-36_Assessment ] .
:SF-36_Assessment 
    rdfs:subClassOf 
        :Assessment ,
        [ owl:onProperty core:hasObservation ; owl:someValuesFrom :SF-36_PhysicalFunctionObservation ] ,
        [ owl:onProperty core:hasObservation ; owl:someValuesFrom :SF-36_SocialFunctionObservation ] ,
        [ owl:onProperty core:hasObservation ; owl:someValuesFrom :SF-36_Role-physicalObservation ] ,
        [ owl:onProperty core:hasObservation ; owl:someValuesFrom :SF-36_Role-emotionalObservation ] ,
        [ owl:onProperty core:hasObservation ; owl:someValuesFrom :SF-36_GeneralHealthObservation ] ,
        [ owl:onProperty core:hasObservation ; owl:someValuesFrom :SF-36_MentalHealthObservation ] ,
        [ owl:onProperty core:hasObservation ; owl:someValuesFrom :SF-36_VitalityObservation ] ,
        [ owl:onProperty core:hasObservation ; owl:someValuesFrom :SF-36_BodilyPainObservation ] ,
        [ owl:onProperty core:hasObservation ; owl:someValuesFrom :SF-36_PhysicalComponentSummaryAssessment ] ,
        [ owl:onProperty core:hasObservation ; owl:someValuesFrom :SF-36_MentalComponentSummaryAssessment ] ;
    rdfs:label "Short Form (SF-36)" ;
    skos:definition """A 36 question survey for measuring health status and outcomes from the patient's point of view. Designed for use in surveys of general and specific populations, health policy evaluations, and clinical practice and research. Measures 8 health concepts: 1) Limitations in physical activities because of health problems, 2) Limitations in usual role activities because of physical health problems, 3) Bodily pain, 4) General health perceptions, 5) Vitality (energy and fatigue), 6) Limitations in social activities because of physical or emotional problems, 7) Limitations in usual role activities because of emotional problems, 8) Mental health (psychological distress and well-being). """ .
:SF-36_PhysicalFunctionObservation 
    rdfs:subClassOf 
        core:SignsAndSymptoms ,
        [ owl:onProperty bridg:PerformedObservation.resultedPerformedObservationResult ; owl:someValuesFrom bridg:PerformedClinicalResult ] ,
        [ owl:onProperty bridg:PerformedActivity.instantiatedDefinedActivity ; owl:hasValue :DefinedSF-36_PhysicalFunctionObservation ] ;
    rdfs:label "PF: Physical Function" ;
    skos:definition """10 items assess physical functio""" .
:DefinedSF-36_PhysicalFunctionObservation rdfs:subClassOf bridg:DefinedObservation ;
    rdfs:label "PF: Physical Function" ;
    skos:definition """10 items assess physical functio""" .

:SF-36_SocialFunctionObservation 
    rdfs:subClassOf 
        core:SignsAndSymptoms ,
        [ owl:onProperty bridg:PerformedObservation.resultedPerformedObservationResult ; owl:someValuesFrom bridg:PerformedClinicalResult ] ,
        [ owl:onProperty bridg:PerformedActivity.instantiatedDefinedActivity ; owl:hasValue :DefinedSF-36_SocialFunctionObservation ] ;
    rdfs:label "SF: Social Function" ;
    skos:definition """2 items to assess social functio""" .
