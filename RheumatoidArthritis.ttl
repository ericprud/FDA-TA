@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .

@prefix dt: <http://www.hl7.org/owl/iso-dt-2.0#> .
@prefix bridg: <http://www.bridgmodel.org/owl#> .

@prefix : <http://www.w3.org/2013/12/FDA-TA/RheumatoidArthritis#> .
@prefix core: <http://www.w3.org/2013/12/FDA-TA/core#> .
@prefix data: <http://www.w3.org/2013/12/FDA-TA/datatypes#> .

<http://www.w3.org/2013/12/FDA-TA/RheumatoidArthritis> a owl:Ontology ;
    owl:imports <http://www.w3.org/2013/12/FDA-TA/core> .

:Assessment rdfs:subClassOf core:Assessment .
:DiseaseActivityScoreEndpoint 
    rdfs:subClassOf 
        core:EfficacyEndpoint ,
        [ owl:onProperty core:evaluates ; owl:someValuesFrom :DAS28_OutcomeAssessment ] ;
    rdfs:label "Disease Activity Score (DAS28)" ;
    skos:definition """Disease Activity Score 28 is a mathematically calculated, continuous, composite endpoint with differential weighting given to each of the following components: tender joint count (28 joints), swollen joint count (28 joints), acute phase reactant, and patient global assessment of arthritis.""" .
:DAS28_OutcomeAssessment 
    rdfs:subClassOf 
        core:SingleOutcomeAssessment ,
        [ owl:onProperty core:beforeIntervention ; owl:someValuesFrom :DAS28_Assessment ] ,
        [ owl:onProperty core:afterIntervention ; owl:someValuesFrom :DAS28_Assessment ] .
:DAS28_Assessment 
    rdfs:subClassOf 
        :Assessment ,
        [ owl:onProperty core:hasObservation ; owl:someValuesFrom :SwollenJointCountObservation ] ,
        [ owl:onProperty core:hasObservation ; owl:someValuesFrom :TenderJointCountObservation ] ,
        [ owl:onProperty core:hasObservation ; owl:someValuesFrom :CReactiveProteinObservation ] ,
        [ owl:onProperty core:hasObservation ; owl:someValuesFrom :ErythrociteSedimentationRateObservation ] ,
        [ owl:onProperty core:hasObservation ; owl:someValuesFrom :PatientGlobalAssessmentOfDiseaseActivityObservation ] ;
    rdfs:label "Disease Activity Score (DAS28)" ;
    skos:definition """Disease Activity Score 28 is a mathematically calculated, continuous, composite endpoint with differential weighting given to each of the following components: tender joint count (28 joints), swollen joint count (28 joints), acute phase reactant, and patient global assessment of arthritis.""" .
:SwollenJointCountObservation 
    rdfs:subClassOf 
        core:QuantitativeMeasurement ,
        [ owl:onProperty bridg:PerformedObservation.resultedPerformedObservationResult ; owl:someValuesFrom bridg:PerformedClinicalResult ] ,
        [ owl:onProperty bridg:PerformedActivity.instantiatedDefinedActivity ; owl:hasValue :DefinedSwollenJointCountObservation ] ;
    rdfs:label "Swollen joint count" ;
    skos:definition """# of joints evaluated for soft-tissue swelling and effusion""" .
:DefinedSwollenJointCountObservation rdfs:subClassOf bridg:DefinedObservation ;
    rdfs:label "Swollen joint count" ;
    skos:definition """# of joints evaluated for soft-tissue swelling and effusion""" .

:TenderJointCountObservation 
    rdfs:subClassOf 
        core:QuantitativeMeasurement ,
        [ owl:onProperty bridg:PerformedObservation.resultedPerformedObservationResult ; owl:someValuesFrom bridg:PerformedClinicalResult ] ,
        [ owl:onProperty bridg:PerformedActivity.instantiatedDefinedActivity ; owl:hasValue :DefinedTenderJointCountObservation ] ;
    rdfs:label "Tender joint count" ;
    skos:definition """ # of joints evaluated for upon pressure or motion""" .
:DefinedTenderJointCountObservation rdfs:subClassOf bridg:DefinedObservation ;
    rdfs:label "Tender joint count" ;
    skos:definition """ # of joints evaluated for upon pressure or motion""" .

:CReactiveProteinObservation 
    rdfs:subClassOf 
        core:QuantitativeMeasurement ,
        [ owl:onProperty bridg:PerformedObservation.resultedPerformedObservationResult ; owl:someValuesFrom bridg:PerformedClinicalResult ] ,
        [ owl:onProperty bridg:PerformedActivity.instantiatedDefinedActivity ; owl:hasValue :DefinedCReactiveProteinObservation ] ;
    rdfs:label "CRP - C reactive peptide Level" ;
    skos:definition """Laboratory Test result for C-reactive protein""" .
:DefinedCReactiveProteinObservation rdfs:subClassOf bridg:DefinedObservation ;
    rdfs:label "CRP - C reactive peptide Level" ;
    skos:definition """Laboratory Test result for C-reactive protein""" .

:ErythrociteSedimentationRateObservation 
    rdfs:subClassOf 
        core:QuantitativeMeasurement ,
        [ owl:onProperty bridg:PerformedObservation.resultedPerformedObservationResult ; owl:someValuesFrom bridg:PerformedClinicalResult ] ,
        [ owl:onProperty bridg:PerformedActivity.instantiatedDefinedActivity ; owl:hasValue :DefinedErythrociteSedimentationRateObservation ] ;
    rdfs:label "ESR - Erythrocyte Sedimentation Rate" ;
    skos:definition """Laboratory Test result for ESR""" .
:DefinedErythrociteSedimentationRateObservation rdfs:subClassOf bridg:DefinedObservation ;
    rdfs:label "ESR - Erythrocyte Sedimentation Rate" ;
    skos:definition """Laboratory Test result for ESR""" .

:PatientGlobalAssessmentOfDiseaseActivityObservation 
    rdfs:subClassOf 
        core:SignsAndSymptoms ,
        [ owl:onProperty bridg:PerformedObservation.resultedPerformedObservationResult ; owl:someValuesFrom bridg:PerformedClinicalResult ] ,
        [ owl:onProperty bridg:PerformedActivity.instantiatedDefinedActivity ; owl:hasValue :DefinedPatientGlobalAssessmentOfDiseaseActivityObservation ] ;
    rdfs:label "Visual Analog Scale (VAS) for Patient Global Assessment " ;
    skos:definition """Scales maybe recorded using a 0-10 scale or a visual analog scale 0-100 mm, or may be recorded using descriptors such as "well" or "poor".""" .
:DefinedPatientGlobalAssessmentOfDiseaseActivityObservation rdfs:subClassOf bridg:DefinedObservation ;
    rdfs:label "Visual Analog Scale (VAS) for Patient Global Assessment " ;
    skos:definition """Scales maybe recorded using a 0-10 scale or a visual analog scale 0-100 mm, or may be recorded using descriptors such as "well" or "poor".""" .

:AmericanCollegeOfRheumatologyEndpoint 
    rdfs:subClassOf 
        core:EfficacyEndpoint ,
        [ owl:onProperty core:evaluates ; owl:someValuesFrom :ACRn_OutcomeAssessment ] ;
    rdfs:label "American College of Rheumatology score (ACRn)" ;
    skos:definition """American College of Rheumatology response criteria is a dichotomou""" .
:ACRn_OutcomeAssessment 
    rdfs:subClassOf 
        core:SingleOutcomeAssessment ,
        [ owl:onProperty core:beforeIntervention ; owl:someValuesFrom :ACRn_Assessment ] ,
        [ owl:onProperty core:afterIntervention ; owl:someValuesFrom :ACRn_Assessment ] .
:ACRn_Assessment 
    rdfs:subClassOf 
        :Assessment ,
        [ owl:onProperty core:hasObservation ; owl:someValuesFrom :SwollenJointCountObservation ] ,
        [ owl:onProperty core:hasObservation ; owl:someValuesFrom :TenderJointCountObservation ] ,
        [ owl:onProperty core:hasObservation ; owl:someValuesFrom :CReactiveProteinObservation ] ,
        [ owl:onProperty core:hasObservation ; owl:someValuesFrom :ErythrociteSedimentationRateObservation ] ,
        [ owl:onProperty core:hasObservation ; owl:someValuesFrom :PatientGlobalAssessmentOfDiseaseActivityObservation ] ,
        [ owl:onProperty core:hasObservation ; owl:someValuesFrom :PatientPainAssessmentObservation ] ,
        [ owl:onProperty core:hasObservation ; owl:someValuesFrom :PhysicianGlobalAssessmentOfDiseaseActivityObservation ] ,
        [ owl:onProperty core:hasObservation ; owl:someValuesFrom :HAQ-DI_Assessment ] ;
    rdfs:label "American College of Rheumatology score (ACRn)" ;
    skos:definition """American College of Rheumatology response criteria is a dichotomou""" .
:PatientPainAssessmentObservation 
    rdfs:subClassOf 
        core:SignsAndSymptoms ,
        [ owl:onProperty bridg:PerformedObservation.resultedPerformedObservationResult ; owl:someValuesFrom bridg:PerformedClinicalResult ] ,
        [ owl:onProperty bridg:PerformedActivity.instantiatedDefinedActivity ; owl:hasValue :DefinedPatientPainAssessmentObservation ] ;
    rdfs:label "Visual Analog Scale (VAS) for Patient Pain Assessment" ;
    skos:definition """May be recorded using a 0-10 scale, or a visual analog scale 0-100 mm.""" .
:DefinedPatientPainAssessmentObservation rdfs:subClassOf bridg:DefinedObservation ;
    rdfs:label "Visual Analog Scale (VAS) for Patient Pain Assessment" ;
    skos:definition """May be recorded using a 0-10 scale, or a visual analog scale 0-100 mm.""" .

:PhysicianGlobalAssessmentOfDiseaseActivityObservation 
    rdfs:subClassOf 
        core:SignsAndSymptoms ,
        [ owl:onProperty bridg:PerformedObservation.resultedPerformedObservationResult ; owl:someValuesFrom bridg:PerformedClinicalResult ] ,
        [ owl:onProperty bridg:PerformedActivity.instantiatedDefinedActivity ; owl:hasValue :DefinedPhysicianGlobalAssessmentOfDiseaseActivityObservation ] ;
    rdfs:label "Visual Analog Scale (VAS) for Physician Global Assessment" ;
    skos:definition """Maybe recorded using a 0-10 scale or a visual analog scale 0-100 mm, or may be recorded using descriptors such as “remission, low disease activity, moderate disease activity or high disease activity”. The Physician global assessment can take into account several factors such as the presence of joint pain, duration of morning stiffness, degree of fatigue, functional status, objective evidence of disease activity (i.e., synovitis, as assessed by tender and swollen joint counts, and the erythrocyte sedimentation rate (ESR) or C-reactive protein (CRP) level), mechanical joint problems (i.e., loss of motion, crepitus, instability, malalignment, and/or deformity), the presence of extraarticular disease, and the presence of radiographic damage.""" .
:DefinedPhysicianGlobalAssessmentOfDiseaseActivityObservation rdfs:subClassOf bridg:DefinedObservation ;
    rdfs:label "Visual Analog Scale (VAS) for Physician Global Assessment" ;
    skos:definition """Maybe recorded using a 0-10 scale or a visual analog scale 0-100 mm, or may be recorded using descriptors such as “remission, low disease activity, moderate disease activity or high disease activity”. The Physician global assessment can take into account several factors such as the presence of joint pain, duration of morning stiffness, degree of fatigue, functional status, objective evidence of disease activity (i.e., synovitis, as assessed by tender and swollen joint counts, and the erythrocyte sedimentation rate (ESR) or C-reactive protein (CRP) level), mechanical joint problems (i.e., loss of motion, crepitus, instability, malalignment, and/or deformity), the presence of extraarticular disease, and the presence of radiographic damage.""" .

:HAQ-DI_Assessment 
    rdfs:subClassOf 
        :Assessment ,
        [ owl:onProperty core:hasObservation ; owl:someValuesFrom :HAQ-DI_DressingAndGroomingObservation ] ,
        [ owl:onProperty core:hasObservation ; owl:someValuesFrom :HAQ-DI_ArisingObservation ] ,
        [ owl:onProperty core:hasObservation ; owl:someValuesFrom :HAQ-DI_EatingObservation ] ,
        [ owl:onProperty core:hasObservation ; owl:someValuesFrom :HAQ-DI_WalkingObservation ] ,
        [ owl:onProperty core:hasObservation ; owl:someValuesFrom :HAQ-DI_HygieneObservation ] ,
        [ owl:onProperty core:hasObservation ; owl:someValuesFrom :HAQ-DI_ReachObservation ] ,
        [ owl:onProperty core:hasObservation ; owl:someValuesFrom :HAQ-DI_GripObservation ] ,
        [ owl:onProperty core:hasObservation ; owl:someValuesFrom :HAQ-DI_ActivitiesObservation ] ;
    rdfs:label "HAQ-DI" ;
    skos:definition """A standardized questionnaire developed by James F. Fries in 1983, which is originally used to measure outcome in patients with Rheumatoid Arthritis. The HAQ-DI is a generic instrument which focuses on self-reported, patient-oriented outcome measures. The questionnaire contains 8 categories of 46 items; dressing and grooming, arising, eating, walking, hygiene, reaching, grip, and activities.""" .
:HAQ-DI_DressingAndGroomingObservation 
    rdfs:subClassOf 
        core:SignsAndSymptoms ,
        [ owl:onProperty bridg:PerformedObservation.resultedPerformedObservationResult ; owl:someValuesFrom bridg:PerformedClinicalResult ] ,
        [ owl:onProperty bridg:PerformedActivity.instantiatedDefinedActivity ; owl:hasValue :DefinedHAQ-DI_DressingAndGroomingObservation ] ;
    rdfs:label "dressing & grooming" ;
    skos:definition """2 questions to assess ability to dress and groom one's self. 2 questions: one question on help of devices and one on help of a person.""" ;
    rdfs:subClassOf [ owl:onProperty dt:CD.CodeSystem ; owl:hasValue <http://example.org/@@FDA-TA> ] ,
                    [ owl:onProperty dt:CD.Coding ; owl:hasValue "refined-null" ] .
:DefinedHAQ-DI_DressingAndGroomingObservation rdfs:subClassOf bridg:DefinedObservation ;
    rdfs:label "dressing & grooming" ;
    skos:definition """2 questions to assess ability to dress and groom one's self. 2 questions: one question on help of devices and one on help of a person.""" ;
    rdfs:subClassOf [ owl:onProperty dt:CD.CodeSystem ; owl:hasValue <http://example.org/@@FDA-TA> ] ,
                    [ owl:onProperty dt:CD.Coding ; owl:hasValue "refined-null" ] .

